@charset "windows-1252";

.timetable { box-sizing: border-box; }

.timetable::after { clear: both; content: ""; display: table; }

.timetable *, .timetable ::before, .timetable ::after { box-sizing: inherit; }

.timetable ul, .timetable li { margin: 0px; padding: 0px; list-style-type: none; }

.timetable aside, .timetable section { float: left; }

.timetable aside { width: 10%; margin-top: 46px; border-right: 5px solid transparent; padding: 0px !important; }

.timetable aside li { padding: 0px 15px; background-color: rgb(236, 178, 178); line-height: 46px; }

.timetable aside li:not(:last-of-type) { border-bottom: 1px solid rgb(255, 255, 255); }

.timetable aside .row-heading { margin-left: 20px; margin-top: -11px; display: inline-block; max-width: 100%; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; overflow-wrap: normal; }

.timetable ul { list-style-type: none; margin: 0px; }

.timetable aside li, .timetable time li { height: 26px; }

.timetable section { width: 90%; overflow-x: scroll; padding: 0px !important; }

.timetable section time { white-space: nowrap; display: inline-block; }

.timetable section header { height: 46px; transform-style: preserve-3d; font-size: 0px; }

.timetable section header::after { clear: both; content: ""; display: table; }

.timetable section header li { font-size: 1rem; overflow: visible; width: 0px; line-height: 46px; display: inline-block; position: relative; }

.timetable section header li:not(:last-of-type) { width: 96px; }

.timetable section header li .time-label { display: block; position: absolute; left: 0px; }

.timetable section header li:not(:first-of-type) .time-label { transform: translateX(-50%); }

.timetable section header li:last-of-type .time-label { transform: translateX(-100%); }

@media only screen and (max-width: 960px) {
  .timetable aside { width: 40%; }
  .timetable section { width: 60%; }
}

.timetable ul.room-timeline { border-left: none; position: relative; overflow: hidden; }

.timetable ul.room-timeline li { position: relative; background-color: lightgray; border: 1px solid black; }

.timetable ul.room-timeline li:nth-of-type(2n+1) { background-color: rgb(253, 253, 253); border: 1px solid black; }

.timetable ul.room-timeline li:first-of-type { border: 1px solid black; }

.timetable ul.room-timeline li:last-of-type { border: 1px solid black; }

.timetable ul.room-timeline li:not(:last-of-type) { border-bottom: none; }

.timetable ul.room-timeline li:first-child .time-entry { height: 44px; }

.timetable ul.room-timeline li::after, .timetable ul.room-timeline li::before { content: ""; position: absolute; z-index: 1; left: 0px; top: 0px; right: 0px; bottom: 0px; }

.timetable ul.room-timeline li::before { background-size: 24px; }

.timetable ul.room-timeline li::after { background-image: linear-gradient(to right, rgb(229, 229, 229), rgb(229, 229, 229) 1px, rgb(244, 244, 244) 1px, rgb(244, 244, 244) 2px, rgb(229, 229, 229) 2px, rgb(229, 229, 229) 3px, transparent 3px, transparent); background-size: 96px; background-position: -2px 0px; }

.timetable .time-entry { background-color: rgb(236, 106, 94); transition: background-color 200ms ease 0s; height: 25px; display: block; position: absolute; z-index: 2; padding: 0px 10px; white-space: normal; overflow: hidden; color: black; border: 1px solid; transform-style: preserve-3d; }

.timetable .time-entry small { position: relative; top: 50%; transform: translateY(-50%); display: block; }

.timetable .time-entry:hover { background-color: rgb(231, 64, 48); }
