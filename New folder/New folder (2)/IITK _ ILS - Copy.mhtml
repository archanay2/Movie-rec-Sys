.bootstrap-dialog .bootstrap-dialog-button-icon { margin-right: 3px; }

.bootstrap-dialog .bootstrap-dialog-close-button { font-size: 20px; float: right; opacity: 0.9; }

.bootstrap-dialog .bootstrap-dialog-close-button:hover { cursor: pointer; opacity: 1; }

.bootstrap-dialog.type-default .modal-header { background-color: rgb(255, 255, 255); }

.bootstrap-dialog.type-default .bootstrap-dialog-title { color: rgb(51, 51, 51); }

.bootstrap-dialog.type-info .modal-header { background-color: rgb(91, 192, 222); }

.bootstrap-dialog.type-primary .modal-header { background-color: rgb(66, 139, 202); }

.bootstrap-dialog.type-success .modal-header { background-color: rgb(92, 184, 92); }

.bootstrap-dialog.type-warning .modal-header { background-color: rgb(240, 173, 78); }

.bootstrap-dialog.type-danger .modal-header { background-color: rgb(217, 83, 79); }

.bootstrap-dialog.size-large .bootstrap-dialog-title { font-size: 24px; }

.bootstrap-dialog.size-large .bootstrap-dialog-close-button { font-size: 30px; }

.bootstrap-dialog.size-large .bootstrap-dialog-message { font-size: 18px; }

.bootstrap-dialog .icon-spin { display: inline-block; animation: 2s linear 0s infinite normal none running spin; }

@-webkit-keyframes spin { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(359deg); }
}

@keyframes spin { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(359deg); }
}
